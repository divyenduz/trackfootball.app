    "@types/react-map-gl": "6.1.7",
    "arg": "5.0.2",
    "eslint": "8.57.0",
    "eslint-config-next": "14.1.3",
    "postcss": "8.5.3",
    "prettier": "2.8.8",
    "vitest": "0.34.6"
  },
  "dependencies": {
